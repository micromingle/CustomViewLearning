        <attr name="strike_color" format="color"></attr>
        <attr name="strike_lenth" format="dimension"></attr>
        <attr name="line_color" format="color"></attr>
        <attr name="line_width" format="dimension"></attr>
        <attr name="bottom_hint_text" format="string"></attr>
        <attr name="overlay_drawable" format="reference"></attr>
        <attr name="trans_height" format="dimension"></attr>
